{
	"click": [
		{
			"selector": "[lizmenu_action=\"load_newmodmaker\"]",
			"function": "fsys.modmaker.app_loader",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "[lizmenu_action=\"load_main_dashboard\"]",
			"function": "fsys.dashboard.app_loader",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "[apptoolbarctg=\"preferences\"] [lizmenu_action=\"exit_app\"]",
			"function": "blfoil_exit_app",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		}
	],

	"change": [
		{
			"selector": "input",
			"function": "window.foil.ux.inputs",
			"pass_event": true,
			"pass_element": true,
			"pass_params": "false"
		}
	],

	"keypress": [
		{
			"selector": "input",
			"function": "window.foil.ux.inputs",
			"pass_event": true,
			"pass_element": true,
			"pass_params": "true"
		}
	]

}